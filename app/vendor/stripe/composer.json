{
  "name": "cloudpayments/cloudpayments-php",
  "description": "CloudPayments PHP Library",
  "keywords": [
    "cloudpayments",
    "payment processing",
    "api"
  ],
  "homepage": "https://cloudpayments.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "CloudPayments and contributors",
      "homepage": "https://github.com/cloudpayments/cloudpayments-php/contributors"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^9.0",
    "friendsofphp/php-cs-fixer": "3.5.0",
    "phpstan/phpstan": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "CloudPayments\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CloudPayments\\": [
        "tests/",
        "tests/CloudPayments/"
      ]
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
